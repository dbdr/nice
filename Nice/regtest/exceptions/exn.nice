// Tests for exception handling

package regtest.exceptions;

class MyEx extends Exception {}

main(args)
{
  String s = null;
  try{
    try{
      s.length();
    }
    finally{
      println("finally");
    }
  }
  catch(NullPointerException e){
    println("NullPointerException");
  }
  
  try{
    throw new MyEx();
  }
  catch(MyEx e){
    println("MyEx");
  }
  catch(Exception e){
    println("Exception");
  }
  System.exit(1);
}
