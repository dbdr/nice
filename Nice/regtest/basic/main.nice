package regtest.basic;

class A
{
  int x;
}

long bidon();
bidon()
{
  long j;
  return j=j;
}

main(args)
{
  A a = new A();
  int i;
  boolean b = false;

  // test how surabondant value is discarded
  
  if (b)
    i = 14;
  else
    bidon();

  if (b)
    i++;
  else
    bidon();

  if (b)
    --i;
  else
    bidon();

  if (b)
    a.x = 17;
  else
    bidon();

  if (b)
    a.x--;
  else
    bidon();
  
  if (b)
    ++a.x;
  else
    bidon();

  String s1;
  fun(String s2) => { s1 = s2; };

  // modification of a variable captured in a lambda
  int j=2;
  println("" + j);
  println(""+(fun()=>--j)());
  println(""+(fun()=>j++)());
  println(""+(fun()=>j=2*j)());

  // Arrays

  Vector<String> v = makeVector(1);
  v.elements.size();
}
