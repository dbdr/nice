package regtest.basic;

class C1 {}
class C2 extends C1 {}

String name(C1);
name(c@C1) = "C1";
name(c@C2) = "C2";

void test_methods();
test_methods()
{
  println ("### Testing methods");
  println(name(new C1()));
  println(name(new C2()));
  ?C1 c = null;
  try{
    println(name(notNull(c)));
  }
  catch(NullPointerException ex){
    println("NULL class");
  }
}
